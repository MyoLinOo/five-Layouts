<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.cardview.widget.CardView
            android:id="@+id/pic1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.1">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/myolinoo"
                    app:layout_constraintVertical_bias="0.25"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/myolinoo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Myo Lin Oo"
            android:textColor="#F2111112"
            android:textSize="18dp"
            android:fontFamily="@font/vidaloka"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/pic1"
            app:layout_constraintLeft_toLeftOf="@id/pic1"
            app:layout_constraintRight_toRightOf="@id/pic1" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Executive Officer"
            android:textColor="#D0111112"
            android:textSize="14dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/myolinoo"
            app:layout_constraintLeft_toLeftOf="@id/pic1"
            app:layout_constraintRight_toRightOf="@id/pic1" />

        <androidx.cardview.widget.CardView
            android:id="@+id/pic2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.5">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/jeck_mar"
                    app:layout_constraintVertical_bias="0.25"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/jackma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jeck Ma"
            android:textColor="#F7111112"
            android:textSize="18dp"
            android:fontFamily="@font/vidaloka"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/pic2"
            app:layout_constraintLeft_toLeftOf="@id/pic2"
            app:layout_constraintRight_toRightOf="@id/pic2" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Marketing"
            android:textColor="#CB111112"
            android:textSize="14dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/jackma"
            app:layout_constraintLeft_toLeftOf="@id/pic2"
            app:layout_constraintRight_toRightOf="@id/pic2" />

        <androidx.cardview.widget.CardView
            android:id="@+id/pic3"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.9">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/iron_man"
                    app:layout_constraintVertical_bias="0.25"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/tony"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tony"
            android:textColor="#F2111112"
            android:fontFamily="@font/vidaloka"
            android:textSize="18dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/pic3"
            app:layout_constraintLeft_toLeftOf="@id/pic3"
            app:layout_constraintRight_toRightOf="@id/pic3" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Business Ananyst"
            android:textColor="#CB111112"
            android:textSize="14dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/tony"
            app:layout_constraintLeft_toLeftOf="@id/pic3"
            app:layout_constraintRight_toRightOf="@id/pic3" />

        <androidx.cardview.widget.CardView
            android:id="@+id/pic4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintTop_toBottomOf="@+id/pic1"
            app:layout_constraintLeft_toLeftOf="@+id/pic1"
            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:src="@drawable/captain_america1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.1" />



        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/pic5"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintTop_toBottomOf="@+id/pic2"
            app:layout_constraintLeft_toLeftOf="@+id/pic2">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/markzuckerburg"
                    app:layout_constraintVertical_bias="0.25"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/pic6"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="50dp"
            app:cardCornerRadius="500dp"
            app:cardElevation="4dp"
            app:layout_constraintTop_toBottomOf="@+id/pic3"
            app:layout_constraintLeft_toLeftOf="@+id/pic3"
            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:src="@drawable/billigate"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.001" />

        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/captain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Captain America"
            android:textAlignment="center"
            android:textColor="#090909"
            android:fontFamily="@font/vidaloka"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="@id/pic4"
            app:layout_constraintRight_toRightOf="@id/pic4"
            app:layout_constraintTop_toBottomOf="@id/pic4" />

        <TextView
                android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UX Designer"
        android:textColor="#C6090909"
        android:textSize="14dp"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/captain"
        app:layout_constraintLeft_toLeftOf="@id/captain"
        app:layout_constraintRight_toRightOf="@id/captain" />
        <TextView
            android:id="@+id/maz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="markzuckerburg"
            android:textColor="#090909"
            android:textSize="17dp"
            android:fontFamily="@font/vidaloka"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/pic5"
            app:layout_constraintLeft_toLeftOf="@id/pic5"
            app:layout_constraintRight_toRightOf="@id/pic5" />
        <TextView
            android:id="@+id/web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Web Developer"
            android:textColor="#C6090909"
            android:textSize="14dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/maz"
            app:layout_constraintLeft_toLeftOf="@id/maz"
            app:layout_constraintRight_toRightOf="@id/maz" />

        <TextView
            android:id="@+id/bill"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Billigate"
            android:textColor="#090909"
            android:textSize="14dp"
            android:fontFamily="@font/vidaloka"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/pic6"
            app:layout_constraintLeft_toLeftOf="@id/pic6"
            app:layout_constraintRight_toRightOf="@id/pic6" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobile Developer"
            android:textColor="#C6090909"
            android:textSize="14dp"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/bill"
            app:layout_constraintLeft_toLeftOf="@id/bill"
            app:layout_constraintRight_toRightOf="@id/bill" />
        <TextView
            android:id="@+id/mession"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mession"
            android:textSize="23dp"
            android:textColor="#000"
            android:fontFamily="@font/vidaloka"
            app:layout_constraintVertical_bias="0.1"
            app:layout_constraintTop_toBottomOf="@id/web"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:layout_width="340dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="8dp"
            android:text="Mission Duis tellus metus, elementum a lectus id, aliquet interdum mauris. Nam bibendum efficitur sollicitudin. Proin eleifend libero velit, nec fringilla dolor finibus quis, nMorbi eu libero pellentesque, rutrum metus quis, blandit est. Fusce bibendum accumsan nisi vulputate feugiat. In fermentum laoreet euismod. Praesent sem nisl, facilisis eget odio at, thoncus scelerisque ipsum. Nulla orci dui, dignissim a risus ut, lobortis porttitor velit. Nulla id lectus metus Maecenas a lorem in odio auctor facilisis non vitae nunc Sed malesuada volutpat massa Praesent cit amet lacinia augue,"
            android:textColor="#000"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mession" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:text="Our Team"
            android:textColor="#000"
            android:fontFamily="@font/vidaloka"
            android:textSize="25dp"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
